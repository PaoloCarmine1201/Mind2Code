{
  "type": "module",
  "name": "mind2code",
  "displayName": "Mind2Code",
  "description": "Chat agent Perspective-Taking for Personalized Code Generation",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Mind2Code.clearChat",
        "title": "Pulisci Chat",
        "category": "Mind2Code"
      },
      {
        "command": "Mind2Code.createGithubContext",
        "title": "Create GitHub Context",
        "category": "Mind2Code"
      },
      {
        "command": "Mind2Code.getGithubContext",
        "title": "Get GitHub Context",
        "category": "Mind2Code"
      },
      {
        "command": "Mind2Code.clearGithubContext",
        "title": "Clear GitHub Context",
        "category": "Mind2Code"
      },
      {
        "command": "Mind2Code.refreshExtension",
        "title": "Refresh Extension",
        "category": "Mind2Code"
      },
      {
        "command": "Mind2Code.startToMProfileQuiz",
        "title": "Start ToM Profile Quiz",
        "category": "Mind2Code"
      },
      {
        "command": "Mind2Code.getToMProfile",
        "title": "Get ToM Profile",
        "category": "Mind2Code"
      },
      {
        "command": "Mind2Code.clearToMProfile",
        "title": "Clear ToM Profile",
        "category": "Mind2Code"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "Mind2Code.clearChat",
          "when": "view == Mind2CodeView",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Mind2CodeSidebar",
          "title": "Mind2Code",
          "icon": "media/web-programming.png"
        }
      ]
    },
    "views": {
      "Mind2CodeSidebar": [
        {
          "type": "webview",
          "id": "Mind2CodeView",
          "name": "Chat with Agent"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0"
  },
  "dependencies": {
    "@huggingface/inference": "^3.10.0",
    "@langchain/core": "^0.3.51",
    "@langchain/langgraph": "^0.2.67",
    "@langchain/openai": "^0.5.10",
    "dotenv": "^16.5.0"
  }
}
